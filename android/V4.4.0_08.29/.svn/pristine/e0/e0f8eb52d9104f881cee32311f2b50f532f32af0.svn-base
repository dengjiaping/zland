public class EBRelation {

    /**
     * 关注了别人
     */
    public static final int TYPE_FOLLOW_TO = 1;

    /**
     * 取消关注别人
     */
    public static final int TYPE_FOLLOW_CANCEL = 2;

    /**
     * 被别人关注
     */
    public static final int TYPE_ADD_FANS = 3;

    /**
     * 移除粉丝
     */
