package com.zhisland.android.blog.aa.eb;

/**
 * 登录注册 event bus
 */
public class EBLogin {

    /**
     * 跳转使用验证码登录
     */
    public static final String LOGIN_BY_VERFIFY_CODE = "LOGIN_BY_VERFIFY_CODE";

    public String action;
    private Object obj;

    public EBLogin(String action) {
        this.action = action;
    }

    public void setObj(Object obj) {
        this.obj = obj;
    }

    public Object getObj() {
        return obj;
    }
}
