package com.zhisland.lib.async.http;

public class Failture {
	private Throwable e;
	private String content;

	public Failture(String content, Throwable e) {
		this.content = content;
		this.e = e;
	}

	public String getContent() {
		return content;
	}

	public Throwable getException() {
		return e;
	}
}
