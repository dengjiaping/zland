<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
            <corners android:radius="@dimen/app_corner_large"/>
            <stroke android:width="@dimen/app_divider_height" android:color="@color/color_f3"/>
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
            <corners android:radius="@dimen/app_corner_large"/>
            <stroke android:width="@dimen/app_divider_height" android:color="@color/color_dc"/>
        </shape>
    </item>

</selector>