<?xml version="1.0" encoding="utf-8"?><resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- img size -->
    
    <dimen name="chat_avatar_size">40dp</dimen>

    <!-- text size -->
    <dimen name="chat_title_size">@dimen/txt_20</dimen>
    <dimen name="chat_name_size">@dimen/txt_18</dimen>
    <dimen name="chat_desc_size">@dimen/txt_16</dimen>

    <!-- padding for whole content and height -->
    <dimen name="chat_padding_left">10dp</dimen>
    <dimen name="chat_padding_right">10dp</dimen>
    <dimen name="chat_padding_top">13dp</dimen>
    <dimen name="chat_padding_bottom">13dp</dimen>
    <dimen name="chat_padding_middle">10dp</dimen>
    <dimen name="post_express_size">20dp</dimen>
    <dimen name="chat_express_size">30dp</dimen>

    <!-- colors -->
    <color name="tag_bg">#fff0f0f0</color>

    <!-- chat -->
    <item name="chat_data_mes" type="id"/>
    <item name="chat_data_listener" type="id"/>
    <item name="chat_data" type="id"/>
    <item name="chat_top" type="id"/>
    <item name="chat_middle" type="id"/>
    <item name="chat_bottom" type="id"/>
    <item name="chat_row_content" type="id"/>
    <item name="chat_row_avator" type="id"/>
    <item name="chat_row_container" type="id"/>
    <item name="chat_row_pro_sending" type="id"/>
    <item name="chat_row_error" type="id"/>
    <item name="chat_row_name" type="id"/>
    <item name="chat_attach" type="id"/>
    <item name="chat_reply" type="id"/>

    <!-- chat messages styles -->

    <style name="chat_block">
        <item name="android:background">@drawable/bg_chat_block</item>
        <item name="android:paddingLeft">13dp</item>
        <item name="android:paddingRight">13dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="chat_top">
        <item name="android:paddingLeft">@dimen/chat_padding_left</item>
        <item name="android:paddingRight">@dimen/chat_padding_right</item>
        <item name="android:paddingTop">@dimen/chat_padding_top</item>
        <item name="android:paddingBottom">@dimen/chat_padding_middle</item>
        <item name="android:background">@drawable/bg_chat_row_top_focus</item>
    </style>

    <style name="chat_middle">
        <item name="android:paddingLeft">@dimen/chat_padding_left</item>
        <item name="android:paddingRight">@dimen/chat_padding_right</item>
        <item name="android:paddingTop">@dimen/chat_padding_middle</item>
        <item name="android:paddingBottom">@dimen/chat_padding_middle</item>
        <item name="android:background">@drawable/bg_chat_row_focus</item>
        <item name="android:duplicateParentState">false</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="chat_bottom">
        <item name="android:paddingLeft">@dimen/chat_padding_left</item>
        <item name="android:paddingRight">@dimen/chat_padding_right</item>
        <item name="android:paddingTop">@dimen/chat_padding_middle</item>
        <item name="android:paddingBottom">@dimen/chat_padding_bottom</item>
        <item name="android:background">@drawable/bg_chat_row_bottom_focus</item>
        <item name="android:duplicateParentState">false</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="chat_divider">
        <item name="android:background">@color/line_gray</item>
    </style>

    <style name="chat_common_img">
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="chat_title" parent="chat_top">
        <item name="android:textSize">@dimen/chat_title_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@null</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="chat_name">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/chat_name_size</item>
        <item name="android:maxLines">2</item>
    </style>

    <style name="chat_desc">
        <item name="android:textColor">@color/txt_light_gray</item>
        <item name="android:textSize">@dimen/chat_desc_size</item>
        <item name="android:maxLines">2</item>
    </style>

    <style name="chat_left_container">
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">40dp</item>
        <item name="android:paddingTop">10dp</item>
    </style>

    <style name="chat_content_left">
        <item name="android:background">@drawable/sel_bg_chat_their</item>
        <item name="android:paddingBottom">11dp</item>
        <item name="android:paddingLeft">18dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingRight">11dp</item>
    </style>

    <style name="chat_content_right">
        <item name="android:background">@drawable/sel_bg_chat_their</item>
        <item name="android:paddingBottom">11dp</item>
        <item name="android:paddingLeft">11dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingRight">18dp</item>
    </style>

    <style name="chat_content_block">
        <item name="android:background">@drawable/bg_chat_info</item>
        <item name="android:paddingBottom">7dp</item>
        <item name="android:paddingLeft">6dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingRight">6dp</item>
    </style>

    <style name="ChattingUIText">
        <item name="android:textSize">@dimen/txt_20</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">15.0dip</item>
        <item name="android:lineSpacingExtra">2.0dip</item>
    </style>

    <style name="ChattingUITitle">
        <item name="android:textSize">@dimen/txt_18</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">left|center_vertical</item>
    </style>

    <style name="ChattingUIDesc">
        <item name="android:textSize">@dimen/txt_18</item>
        <item name="android:textColor">@color/txt_light_gray</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">left|center_vertical</item>
    </style>
    
    <style name="ChattingItemSourceTextView">
        <item name="android:textSize">11.0dip</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">3.0dip</item>
    </style>

    <!-- for style none chat row, such as group feed -->
    <style name="chat_content">
        <item name="android:paddingBottom">0dp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingRight">8dp</item>
    </style>

    <!--<style name="bg_toolbar">-->
        <!--<item name="android:background">@color/chat_toolbar</item>-->
        <!--<item name="android:paddingTop">@dimen/app_line_height</item>-->
        <!--<item name="android:gravity">center_vertical</item>-->
    <!--</style>-->

</resources>