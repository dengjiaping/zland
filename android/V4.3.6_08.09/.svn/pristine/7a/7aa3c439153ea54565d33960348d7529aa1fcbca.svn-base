package com.zhisland.android.blog.contacts.eb;

public class EBContacts {

	/**
	 * 求邀请发生变化
	 */
	public static final int TYPE_INVITATION_REQUEST_CHANGE = 1;
	
	private int type;
	private Object obj;

	public EBContacts(int type, Object obj) {
		this.type = type;
		this.obj = obj;
	}

	public int getType() {
		return type;
	}

	public Object getData() {
		return obj;
	}

}
